#include<stdio.h>
int main()
{
int opt;
printf("Enter any number from 1 to 5 to select food item\n");
scanf("%d",&opt);
switch(opt)
{
case 1:printf("Food item-Pizza\nPrice-Rs.239\n");
break;
case 2:printf("Food item-Burger\nPrice-Rs.129\n");
break;
case 3:printf("Food item-Pasta\nPrice-Rs.179\n");
break;
case 4:printf("Food item-Fench Fries\nPrice-Rs.99\n");
break;
case 5:printf("Food item-Sandwich\nPrice-Rs.149\n");
break;
default:printf("Invalid choice\n");
}
return 0;
}
